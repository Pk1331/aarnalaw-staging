{
  "extends": [
    "next/core-web-vitals",
    "plugin:tailwindcss/recommended",
    "prettier"
  ],
  "settings": {
    "tailwindcss": {
      "callees": [
        "twMerge",
        "createTheme"
      ],
      "classRegex": "^(class(Name)|theme)?$"
    }
  },
  "rules": {
    "no-console": "off",
    "no-unused-vars": "warn"
  }
}